{"ast":null,"code":"import Api from \"@/services/api\";\nimport fileDownload from \"js-file-download\";\nexport default {\n  methods: {\n    backupandDownload(e) {\n      e.target.disable = true;\n      Api.get(\"/dump\").then(res => {\n        fileDownload(res.data, `${Date.now()}.sql`);\n        e.target.disable = false;\n      }).catch(err => {\n        console.log(err);\n        e.target.disable = false;\n      });\n    },\n\n    Downloadlast(e) {\n      e.target.disable = true;\n      Api.get(\"/lastdump\").then(res => {\n        console.log(res);\n        fileDownload(res.data, `${Date.now()}.sql`);\n        e.target.disable = false;\n      }).catch(err => {\n        console.log(err);\n        e.target.disable = false;\n      });\n    }\n\n  }\n};","map":{"version":3,"sources":["C:\\Users\\Dell\\Desktop\\vost-app\\src\\views\\Parametres.vue"],"names":[],"mappings":"AAaA,OAAO,GAAP,MAAgB,gBAAhB;AACA,OAAO,YAAP,MAAyB,kBAAzB;AACA,eAAe;AACX,EAAA,OAAO,EAAC;AACJ,IAAA,iBAAiB,CAAC,CAAD,EAAG;AAChB,MAAA,CAAC,CAAC,MAAF,CAAS,OAAT,GAAmB,IAAnB;AACA,MAAA,GAAG,CAAC,GAAJ,CAAQ,OAAR,EAAiB,IAAjB,CAAuB,GAAD,IAAO;AACzB,QAAA,YAAY,CAAC,GAAG,CAAC,IAAL,EAAY,GAAE,IAAI,CAAC,GAAL,EAAW,MAAzB,CAAZ;AACA,QAAA,CAAC,CAAC,MAAF,CAAS,OAAT,GAAmB,KAAnB;AACH,OAHD,EAGG,KAHH,CAGU,GAAD,IAAO;AACZ,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,QAAA,CAAC,CAAC,MAAF,CAAS,OAAT,GAAmB,KAAnB;AACH,OAND;AAOH,KAVG;;AAWJ,IAAA,YAAY,CAAC,CAAD,EAAG;AACX,MAAA,CAAC,CAAC,MAAF,CAAS,OAAT,GAAmB,IAAnB;AACA,MAAA,GAAG,CAAC,GAAJ,CAAQ,WAAR,EAAqB,IAArB,CAA2B,GAAD,IAAO;AAC7B,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,QAAA,YAAY,CAAC,GAAG,CAAC,IAAL,EAAY,GAAE,IAAI,CAAC,GAAL,EAAW,MAAzB,CAAZ;AACA,QAAA,CAAC,CAAC,MAAF,CAAS,OAAT,GAAmB,KAAnB;AACH,OAJD,EAIG,KAJH,CAIU,GAAD,IAAO;AACZ,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACA,QAAA,CAAC,CAAC,MAAF,CAAS,OAAT,GAAmB,KAAnB;AACH,OAPD;AAQH;;AArBG;AADG,CAAf","sourcesContent":["<template>\r\n  <div\r\n\t\tclass=\"flex flex-col w-full h-screen items-center bg-gray-700 overflow-hidden space-y-1\"\r\n\t>\r\n\t\t<!-- setting page -->\r\