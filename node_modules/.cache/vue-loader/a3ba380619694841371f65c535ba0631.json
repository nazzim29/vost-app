ps: {\r\n        production: {\r\n            type: Object,\r\n            required: true,\r\n        },\r\n    },\r\n    data(){\r\n        return {\r\n            api_url: process.env.VUE_APP_API_URL+\"/uploads/products/\",\r\n        }\r\n    },\r\n    methods:{\r\n        formatDate(date){\r\n            date = new Date(date);\r\n            let day = date.getDate();\r\n            let month = date.getMonth() + 1;\r\n            let year = date.getFullYear();\r\n            return day + '/' + month + '/' + year;\r\n\r\n        }\r\n    },\r\n    mounted(){\r\n        console.log(this.production)\r\n    }\r\n\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}